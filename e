let worldData=null,subData=null,records={people:{},places:{}};
let selCountry=null,hovered=null,panelMode="people",activeTab="all",newEntry={who:"",name:"",note:""};
let rot=[0,-20,0],scale=1,autoRotate=true,drag=null,resumeT=null,dims={w:960,h:700};
const canvas=$("CV"),proj=d3.geoOrthographic().clipAngle(90).precision(0.3);

function starCol(temp){if(temp<3800)return"255,185,125";if(temp<5000)return"255,225,185";if(temp<6500)return"255,248,240";if(temp<8500)return"215,228,255";return"175,205,255";}
const STARS=mk(650,()=>{const mag=Math.random();return{x:Math.random(),y:Math.random(),r:mag<0.04?Math.random()*1.6+0.9:mag<0.25?Math.random()*0.8+0.3:Math.random()*0.35+0.08,a:Math.random()*0.5+0.2,sp:Math.random()*0.008+0.001,off:Math.random()*Math.PI*2,temp:Math.random()<0.08?3200+Math.random()*2000:Math.random()<0.15?8500+Math.random()*3500:5200+Math.random()*2000}});
const MILKYWAY=mk(900,()=>{const a=Math.random()*Math.PI*2,spread=(Math.random()-0.5)*0.16;return{a,r:0.44+Math.sin(a*2.3)*0.06+spread,b:Math.random()*0.022+0.004,s:Math.random()*0.5+0.1}});
const DUST=mk(180,()=>({x:Math.random(),y:Math.random(),r:Math.random()*0.4+0.06,a:Math.random()*0.04+0.005,dx:(Math.random()-0.5)*0.000015,dy:(Math.random()-0.5)*0.000015}));
const SHOOTING=mk(4,()=>({active:false,x:0,y:0,vx:0,vy:0,life:0,maxLife:0,trail:[]}));
const LIGHTS=mk(260,()=>({lon:Math.random()*360-180,lat:Math.random()*130-65,b:Math.random()*0.5+0.3,r:Math.random()*1.3+0.3,fl:Math.random()*0.012+0.002,off:Math.random()*Math.PI*2}));
const CLOUDS=mk(38,()=>{const clon=Math.random()*360-180,clat=(Math.random()-0.5)*110;return{lon:clon,lat:clat,puffs:mk(Math.floor(Math.random()*7)+3,()=>({dlon:(Math.random()-0.5)*12,dlat:(Math.random()-0.5)*5,r:Math.random()*0.025+0.01,op:Math.random()*0.055+0.02})),speed:(Math.random()-0.5)*0.007};});
const L1=["#3a7d48","#438a52","#3c7242","#4e8c50","#478248","#528a4c","#3f7a44","#4a8650","#447e4a","#4d8850"];
const L2=["#6d945a","#7a9c60","#859e5c","#768c54","#8a9a58","#7d9456","#929c5a","#6a8a52","#80965c","#748e50"];
const L3=["#9a9060","#a89868","#b5a070","#8e8558","#a49262","#98885c","#aa9a68","#8c8254","#a09060","#b0a06a"];
const L4=["#888878","#7a7a6c","#929282","#6e6e60","#848474","#7c7c6e","#8a8a7a","#767668","#828272","#6c6c5e"];
const PAL=[L1,L2,L3,L4];
function lc(n){let h=0;for(let i=0;i<(n||"").length;i++)h=((h<<5)-h+n.charCodeAt(i))|0;const a=Math.abs(h);return PAL[a%4][(a>>4)%10]}

function cS(){
  let ep=0,sp=0,epl=0,spl=0;
  const ec=new Set(),sc=new Set(),cc=new Set();
  Object.entries(records.people||{}).forEach(([k,a])=>{
    a.forEach(x=>{
      if(x.who==="eissa"){ep++;ec.add(k);}else{sp++;sc.add(k);}
      cc.add(k);
    });
  });
  Object.entries(records.places||{}).forEach(([k,a])=>{
    a.forEach(x=>{
      if(x.who==="eissa"){epl++;ec.add(k);}else{spl++;sc.add(k);}
      cc.add(k);
    });
  });
  return{
    eissaPeople:ep, eissaPlaces:epl, eissaCountries:ec.size,
    serenaPeople:sp, serenaPlaces:spl, serenaCountries:sc.size,
    totalPeople:ep+sp, totalPlaces:epl+spl, totalCountries:cc.size
  };
}
function rS(){
  const s=cS();
  $("SB").innerHTML=`
  <div class="sb">
    <div class="sb-person eissa">
      <div class="sb-person-header">
        <img src="${FA}" class="arg-flag" alt="AR">
        <span class="sb-name arg-grad">Eissa</span>
      </div>
      <div class="sb-stats">
        <div class="sb-stat"><span class="sb-stat-val">${s.eissaPeople}</span><span class="sb-stat-lbl">People</span></div>
        <div class="sb-stat"><span class="sb-stat-val">${s.eissaPlaces}</span><span class="sb-stat-lbl">Places</span></div>
        <div class="sb-stat"><span class="sb-stat-val">${s.eissaCountries}</span><span class="sb-stat-lbl">Countries</span></div>
      </div>
    </div>
    <div class="sb-div"></div>
    <div class="sb-person serena">
      <div class="sb-person-header">
        <img src="${qcUri}" class="qc-flag" alt="QC">
        <span class="sb-name ser-grad">Serena</span>
      </div>
      <div class="sb-stats">
        <div class="sb-stat"><span class="sb-stat-val">${s.serenaPeople}</span><span class="sb-stat-lbl">People</span></div>
        <div class="sb-stat"><span class="sb-stat-val">${s.serenaPlaces}</span><span class="sb-stat-lbl">Places</span></div>
        <div class="sb-stat"><span class="sb-stat-val">${s.serenaCountries}</span><span class="sb-stat-lbl">Countries</span></div>
      </div>
    </div>
    <div class="sb-div"></div>
    <div class="sb-shared">
      <div class="sb-shared-header">Together</div>
      <div class="sb-stats">
        <div class="sb-stat"><span class="sb-stat-val">${s.totalPeople}</span><span class="sb-stat-lbl">People</span></div>
        <div class="sb-stat"><span class="sb-stat-val">${s.totalPlaces}</span><span class="sb-stat-lbl">Places</span></div>
        <div class="sb-stat"><span class="sb-stat-val">${s.totalCountries}</span><span class="sb-stat-lbl">Countries</span></div>
      </div>
    </div>
  </div>`;
}

function resize(){const b=document.body;dims={w:b.clientWidth,h:b.clientHeight};const dpr=devicePixelRatio||1;canvas.width=dims.w*dpr;canvas.height=dims.h*dpr;canvas.style.width=dims.w+"px";canvas.style.height=dims.h+"px";}
addEventListener("resize",resize);

// Countries with sub-national breakdown ‚Äî clicking them opens country first
const SUB_COUNTRIES=new Set(["USA","Canada","Argentina"]);
let subModeCountry=null; // when set, ht() returns sub-regions for this country

function ht(mx,my){
  if(!worldData)return null;
  const c=proj.invert([mx,my]);
  if(!c||isNaN(c[0]))return null;
  // If we're in sub-mode for a specific country, return sub-regions only for that country
  if(subModeCountry&&subData?.features){
    for(const f of subData.features){
      const iso=f.properties?.iso_a2||"";
      const matchesCountry=(subModeCountry==="USA"&&iso==="US")||(subModeCountry==="Canada"&&iso==="CA")||(subModeCountry==="Argentina"&&iso==="AR");
      if(matchesCountry&&d3.geoContains(f,c)){
        return f.properties?.name||null;
      }
    }
    return null; // clicking outside sub-regions in sub-mode does nothing
  }
  // Normal mode: check sub-national features BUT skip the big-3 countries (return country name instead)
  if(subData?.features){
    for(const f of subData.features){
      if(d3.geoContains(f,c)){
        const iso=f.properties?.iso_a2||"";
        // For big-3 countries, return country name so first click = country panel
        if(iso==="US")return "USA";
        if(iso==="CA")return "Canada";
        if(iso==="AR")return "Argentina";
        return f.properties?.name||null;
      }
    }
  }
  // Fall back to country
  for(const f of worldData.features)if(d3.geoContains(f,c))return f.properties?.name||null;
  return null;
}
canvas.addEventListener("pointerdown",e=>{drag={x:e.clientX,y:e.clientY,rot:[...rot],moved:false};autoRotate=false;});
canvas.addEventListener("pointermove",e=>{if(drag){const dx=e.clientX-drag.x,dy=e.clientY-drag.y;if(Math.abs(dx)>2||Math.abs(dy)>2)drag.moved=true;rot=[drag.rot[0]+dx*0.3,Math.max(-85,Math.min(85,drag.rot[1]-dy*0.3)),0];}const name=ht(e.clientX,e.clientY);if(name!==hovered){hovered=name;if(name&&!selCountry){$("TN").textContent=name;const cnt=((records.people?.[name]||[]).length+(records.places?.[name]||[]).length);$("TC").textContent=subModeCountry?(cnt>0?`${cnt} ${cnt===1?"entry":"entries"} logged`:"Click to log a visit"):(cnt>0?`${cnt} ${cnt===1?"entry":"entries"} logged`:"Click to start logging");$("TT").classList.add("vi");}else $("TT").classList.remove("vi");}});
canvas.addEventListener("pointerup",e=>{if(drag&&!drag.moved){const name=ht(e.clientX,e.clientY);if(name)oP(name);}drag=null;clearTimeout(resumeT);resumeT=setTimeout(()=>{autoRotate=true;},4000);});
canvas.addEventListener("pointerleave",()=>{drag=null;hovered=null;$("TT").classList.remove("vi");});
canvas.addEventListener("wheel",e=>{e.preventDefault();scale=Math.max(0.6,Math.min(6,scale*(e.deltaY>0?0.93:1.07)));},{passive:false});

function oP(name){
  // If clicking a sub-region while in sub-mode, open that sub-region
  selCountry=name;
  newEntry={who:"",name:"",note:"",lon:null,lat:null,customName:""};
  panelMode="people";activeTab="all";
  $("IN").style.opacity="0";
  $("TT").classList.remove("vi");
  $("OV").classList.add("op");
  $("OV").classList.remove("cl");
  requestAnimationFrame(()=>$("OV").classList.add("vi"));
  rP();
}
function cP(){
  clearTimeout(_drillTimer);
  $("OV").classList.add("cl");$("OV").classList.remove("vi");
  setTimeout(()=>{
    $("OV").classList.remove("op","cl");
    selCountry=null;
    if(!subModeCountry)$("SUBPILL").classList.remove("vis"); // only hide pill if not in sub-mode
    $("IN").style.opacity="1";
  },400);
}
$("BK").onclick=cP;$("CB").onclick=cP;

let _drillTimer=null;
function rP(){
  if(!selCountry)return;
  $("PC").textContent=selCountry;
  $("MP").textContent=(records.people?.[selCountry]||[]).length;
  $("ML").textContent=(records.places?.[selCountry]||[]).length;
  rM();rF();rT();rE();
  // Drill-down banner ‚Äî only for the big-3 countries
  clearTimeout(_drillTimer);
  const drill=$("DRILL");
  const isBig3=SUB_COUNTRIES.has(selCountry);
  if(!isBig3){drill.style.display="none";return;}
  const labels={USA:"States",Canada:"Provinces",Argentina:"Provincias"};
  const flags={USA:"üá∫üá∏",Canada:"üá®üá¶",Argentina:"üá¶üá∑"};
  const label=labels[selCountry]||"Regions";
  $("DRBTN").innerHTML=`Explore ${label} <span>‚Üí click any region on the globe</span>`;
  // Reset to locked
  drill.style.display="";
  drill.classList.remove("unlocked");
  $("DRCT").textContent="3";
  const bar=$("DRBAR");
  bar.style.transition="none";
  bar.style.transform="scaleX(1)";
  requestAnimationFrame(()=>requestAnimationFrame(()=>{
    bar.style.transition="transform 3s linear";
    bar.style.transform="scaleX(0)";
  }));
  // Countdown ticks
  let n=3;
  const tick=()=>{n--;if(n>0){$("DRCT").textContent=n;_drillTimer=setTimeout(tick,1000);}};
  _drillTimer=setTimeout(tick,1000);
  // Unlock at 3s
  _drillTimer=setTimeout(()=>{
    drill.classList.add("unlocked");
    $("DRBTN").onclick=()=>{
      const c=selCountry;
      cP();
      setTimeout(()=>{
        subModeCountry=c;
        $("SPFLAG").textContent=flags[c]||"";
        $("SPNAME").textContent=c;
        $("SUBPILL").classList.add("vis");
      },420);
    };
  },3000);
}
// Back pill ‚Äî exit sub-mode, re-open country panel
$("SUBPILL").onclick=()=>{
  const c=subModeCountry;
  subModeCountry=null;
  $("SUBPILL").classList.remove("vis");
  if(c)setTimeout(()=>oP(c),50);
};
function rM(){$("MR").innerHTML=[{id:"people",l:"People Met",i:"ü§ù"},{id:"places",l:"Places Visited",i:"üìç"}].map(m=>{const a=panelMode===m.id;return`<button class="mb ${a?"a":""}" data-m="${m.id}" style="border-color:${a?"rgba(255,255,255,0.08)":"rgba(255,255,255,0.025)"};background:${a?"rgba(255,255,255,0.03)":"rgba(255,255,255,0.005)"};color:${a?"#fff":"rgba(255,255,255,0.4)"}">${m.i} ${m.l}</button>`}).join("");document.querySelectorAll(".mb").forEach(b=>b.onclick=()=>{panelMode=b.dataset.m;activeTab="all";newEntry={who:"",name:"",note:"",lon:null,lat:null,customName:""};rP();});}
function rF(){
  const isP=panelMode==="people",eA=newEntry.who==="eissa",sA=newEntry.who==="serena";
  const plcCount=(records.places?.[selCountry]||[]).length;
  const pinCapReached=!isP&&plcCount>=7;
  const cities=!isP?CITY_DB[selCountry]||[]:[];
  const isCustom=newEntry.name==="__custom__"||(!cities.length&&!isP);

  // Build person selector HTML
  const personHTML=`<div class="pr">
    <button class="ps ${eA?"a":""}" data-w="eissa" style="border-color:${eA?"rgba(255,255,255,0.3)":"rgba(255,255,255,0.025)"};background:${eA?"rgba(255,255,255,0.06)":"rgba(255,255,255,0.006)"};color:${eA?"#fff":"rgba(255,255,255,0.45)"}">
      <img src="${FA}" class="arg-flag" alt=""> <span class="arg-grad">Eissa</span>
    </button>
    <button class="ps ${sA?"a":""}" data-w="serena" style="border-color:${sA?"rgba(255,255,255,0.3)":"rgba(255,255,255,0.025)"};background:${sA?"rgba(255,255,255,0.06)":"rgba(255,255,255,0.006)"};color:${sA?"#fff":"rgba(255,255,255,0.45)"}">
      <img src="${qcUri}" class="qc-flag" alt="QC"> <span class="ser-grad">Serena</span>
    </button>
  </div>`;

  // Build pin counter for places mode
  const pinCounterHTML=!isP?`<div style="font-size:10px;color:rgba(255,255,255,0.3);letter-spacing:1px;margin-bottom:12px;display:flex;align-items:center;gap:6px">
    <span>${[1,2,3,4,5,6,7].map(i=>`<span style="display:inline-block;width:7px;height:7px;border-radius:50%;margin-right:2px;background:${i<=plcCount?"rgba(117,170,219,0.85)":"rgba(255,255,255,0.1)"};box-shadow:${i<=plcCount?"0 0 4px rgba(117,170,219,0.5)":"none"}">&nbsp;</span>`).join("")}</span>
    <span style="color:rgba(255,255,255,0.2)">${plcCount}/7 pins</span>
  </div>`:"";

  // Build place input: dropdown if cities exist, plain text for people or custom
  let placeInputHTML="";
  if(!isP&&!pinCapReached){
    if(cities.length){
      const selVal=newEntry.name&&newEntry.name!=="__custom__"?newEntry.name:"";
      const opts=cities.map(c=>`<option value="${esc(c[0])}" data-lon="${c[1]}" data-lat="${c[2]}" ${selVal===c[0]?"selected":""}>${c[0]}</option>`).join("");
      placeInputHTML=`
        <select class="sf" id="PD">
          <option value="" disabled ${!selVal?"selected":""}>üìç Choose a place‚Ä¶</option>
          ${opts}
          <option value="__custom__" ${newEntry.name==="__custom__"?"selected":""}>‚úèÔ∏è Custom place‚Ä¶</option>
        </select>
        ${isCustom?`<input class="if" id="EN" value="${esc(newEntry.customName||"")}" placeholder="Type a place name‚Ä¶" style="margin-top:-4px">`:""}`;
    } else {
      // No cities in DB for this region ‚Äî fallback to plain text
      placeInputHTML=`<input class="if" id="EN" value="${esc(newEntry.name)}" placeholder="Where did you go?">`;
    }
  } else if(isP&&!pinCapReached){
    placeInputHTML=`<input class="if" id="EN" value="${esc(newEntry.name)}" placeholder="Who did you meet?">`;
  }

  const ready=newEntry.who&&(isP?newEntry.name:(newEntry.name&&newEntry.name!=="__custom__")||(isCustom&&newEntry.customName));
  const noteHTML=!pinCapReached?`<textarea class="tf" id="NO" rows="2" placeholder="${isP?"Story, context, how you met...":"Memories, highlights, what you loved..."}">${esc(newEntry.note)}</textarea>`:"";
  const btnHTML=!pinCapReached?`<button class="ab ${ready?"en":""}" id="AB" style="${ready?"background:linear-gradient(135deg,#e8976c,#d4823a,#c87030);color:#080e18;box-shadow:0 6px 28px rgba(232,151,108,0.3)":""}">+ ${isP?"Log Person":"Log Place"}</button>`:"";
  const capHTML=pinCapReached?`<div style="font-size:11px;color:rgba(117,170,219,0.6);letter-spacing:.5px;text-align:center;padding:12px;background:rgba(117,170,219,0.06);border-radius:10px;margin-bottom:12px">üìç Max 7 pins per country reached</div>`:"";

  $("FS").innerHTML=`<div class="fl">${isP?"New Encounter":"New Place"}</div>${personHTML}${pinCounterHTML}${capHTML}${placeInputHTML}${noteHTML}${btnHTML}`;

  // Re-attach person selector events
  document.querySelectorAll(".ps").forEach(b=>b.onclick=()=>{newEntry.who=b.dataset.w;rF();});

  // Dropdown change handler
  const pd=$("PD");
  if(pd){
    pd.onchange=()=>{
      const opt=pd.options[pd.selectedIndex];
      newEntry.name=opt.value;
      newEntry.lon=opt.dataset.lon?parseFloat(opt.dataset.lon):null;
      newEntry.lat=opt.dataset.lat?parseFloat(opt.dataset.lat):null;
      newEntry.customName="";
      rF(); // re-render to show/hide custom input
    };
  }

  // Custom text input
  const en=$("EN");
  if(en){
    en.oninput=()=>{
      if(isP){newEntry.name=en.value;}
      else{newEntry.customName=en.value;newEntry.lon=null;newEntry.lat=null;}
      const ab=$("AB");
      const rdy=newEntry.who&&(isP?newEntry.name:(newEntry.name&&newEntry.name!=="__custom__")||(isCustom&&newEntry.customName));
      if(rdy){ab.classList.add("en");ab.style.cssText="background:linear-gradient(135deg,#e8976c,#d4823a,#c87030);color:#080e18;box-shadow:0 6px 28px rgba(232,151,108,0.3)";}
      else{ab.classList.remove("en");ab.style.cssText="";}
    };
    en.onkeydown=e=>{if(e.key==="Enter")aE();};
    requestAnimationFrame(()=>en.focus());
  }

  const no=$("NO");
  if(no) no.oninput=()=>{newEntry.note=no.value;};

  const ab=$("AB");
  if(ab) ab.onclick=aE;
}
async function aE(){
  if(!newEntry.who||!selCountry)return;
  const t=panelMode==="people"?"people":"places";
  const isP=t==="people";
  const cities=!isP?CITY_DB[selCountry]||[]:[];

  // Resolve the final display name and coords
  let finalName,finalLon=null,finalLat=null;
  if(isP){
    finalName=newEntry.name;
  } else if(newEntry.name==="__custom__"||!cities.length){
    finalName=newEntry.customName||newEntry.name;
  } else {
    finalName=newEntry.name;
    finalLon=newEntry.lon??null;
    finalLat=newEntry.lat??null;
    // If user picked from dropdown, retrieve coords from CITY_DB in case not yet on newEntry
    if(finalName&&(finalLon===null)){
      const match=cities.find(c=>c[0]===finalName);
      if(match){finalLon=match[1];finalLat=match[2];}
    }
  }
  if(!finalName)return;
  if(t==="places"&&(records.places?.[selCountry]||[]).length>=7)return;

  if(!records[t])records[t]={};
  if(!records[t][selCountry])records[t][selCountry]=[];
  const entry={id:Date.now()+"_"+Math.random().toString(36).slice(2,6),who:newEntry.who,name:finalName,note:newEntry.note||"",ts:Date.now()};
  if(finalLon!==null)entry.lon=finalLon;
  if(finalLat!==null)entry.lat=finalLat;
  records[t][selCountry].push(entry);
  if(usingSupa)await insertToSupabase(t,selCountry,entry);else await saveLocal();
  rS();
  newEntry={who:newEntry.who,name:"",note:"",lon:null,lat:null,customName:""};
  rP();
}
async function dE(type,country,id){if(!records[type]?.[country])return;records[type][country]=records[type][country].filter(e=>e.id!==id);if(!records[type][country].length)delete records[type][country];if(usingSupa)await deleteFromSupabase(id);else await saveLocal();rS();rP();}
function rT(){$("TR").innerHTML=[{id:"all",l:"All"},{id:"eissa",l:`<img src="${FA}" class="arg-flag" alt=""> <span class="arg-grad">Eissa</span>`},{id:"serena",l:`<img src="${qcUri}" class="qc-flag" alt="QC"> <span class="ser-grad">Serena</span>`}].map(t=>{const a=activeTab===t.id;return`<button class="tp ${a?"a":""}" data-t="${t.id}" style="background:${a?"rgba(255,255,255,0.06)":"transparent"};color:${a?"#fff":"rgba(255,255,255,0.4)"}">${t.l}</button>`}).join("");document.querySelectorAll(".tp").forEach(b=>b.onclick=()=>{activeTab=b.dataset.t;rT();rE();});}
function rE(){const type=panelMode==="people"?"people":"places";const src=records[type]?.[selCountry]||[];let entries=activeTab==="all"?src:src.filter(e=>e.who===activeTab);entries=[...entries].sort((a,b)=>b.ts-a.ts);const c=$("EC");
if(!entries.length){c.innerHTML=`<div class="es"><div class="ic">${panelMode==="people"?"ü§ù":"üó∫Ô∏è"}</div><h3>${panelMode==="people"?"No people logged yet":"No places logged yet"}</h3><p>${panelMode==="people"?`Record someone you met from ${selCountry}`:`Add a place you visited in ${selCountry}`}</p></div>`;return;}
c.innerHTML=`<div class="el">${entries.map((e,i)=>{const isE=e.who==="eissa";return`<div class="ec" style="border-left-color:${isE?"rgba(255,150,150,0.15)":"rgba(150,150,255,0.15)"};animation-delay:${i*0.05}s"><div style="display:flex;justify-content:space-between;align-items:flex-start"><div style="flex:1;min-width:0"><div style="display:flex;align-items:center;gap:12px;margin-bottom:${e.note?"10":"0"}px;flex-wrap:wrap"><span class="en3">${esc(e.name)}</span><span class="eb" style="background:rgba(255,255,255,0.04);color:#fff;border:1px solid rgba(255,255,255,0.06)">${isE?`<img src="${FA}" class="arg-flag" alt=""> <span class="arg-grad">Eissa</span>`:`<img src="${qcUri}" class="qc-flag" alt="QC"> <span class="ser-grad">Serena</span>`}</span></div>${e.note?`<div class="eno">${esc(e.note)}</div>`:""}</div><button class="db" data-t="${type}" data-id="${e.id}">√ó</button></div></div>`}).join("")}</div>`;
c.querySelectorAll(".db").forEach(b=>b.onclick=()=>dE(b.dataset.t,selCountry,b.dataset.id));}
function esc(s){return(s||"").replace(/&/g,"&amp;").replace(/</g,"&lt;").replace(/>/g,"&gt;").replace(/"/g,"&quot;")}

// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê RENDER LOOP ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
function startR(){
const dpr=devicePixelRatio||1;
const grainC=document.createElement("canvas");grainC.width=256;grainC.height=256;
const gx=grainC.getContext("2d"),gid=gx.createImageData(256,256);
for(let i=0;i<gid.data.length;i+=4){const v=Math.random()*255;gid.data[i]=gid.data[i+1]=gid.data[i+2]=v;gid.data[i+3]=5;}
gx.putImageData(gid,0,0);
let frame=0;
(function render(now){
requestAnimationFrame(render);frame++;
const{w,h}=dims,ctx=canvas.getContext("2d");
ctx.setTransform(dpr,0,0,dpr,0,0);
const bR=Math.min(w,h)*0.4,radius=bR*scale,cx=w/2,cy=h/2;
proj.scale(radius).translate([cx,cy]).rotate(rot);
if(autoRotate&&!drag)rot=[rot[0]+0.038,rot[1],0];
const path=d3.geoPath(proj,ctx),t=now*0.001;
const bg=ctx.createRadialGradient(cx*0.55,cy*0.3,0,cx,cy,Math.max(w,h));
bg.addColorStop(0,"#0a1020");bg.addColorStop(0.25,"#070c18");bg.addColorStop(0.55,"#040812");bg.addColorStop(1,"#020406");
