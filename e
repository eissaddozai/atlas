[[0.12,0.2,0.3,"30,50,100"],[0.82,0.72,0.22,"50,22,70"],[0.45,0.88,0.18,"22,45,60"],[0.88,0.12,0.13,"40,18,55"]].forEach(([nx,ny,nr,rgb])=>{const ng=ctx.createRadialGradient(w*nx,h*ny,0,w*nx,h*ny,w*nr);ng.addColorStop(0,`rgba(${rgb},0.025)`);ng.addColorStop(0.5,`rgba(${rgb},0.008)`);ng.addColorStop(1,"transparent");ctx.fillStyle=ng;ctx.fillRect(0,0,w,h);});
const mwA=0.38;ctx.save();MILKYWAY.forEach(p=>{const px=cx+Math.cos(p.a+mwA)*w*p.r;const py=cy*0.45+Math.sin(p.a+mwA)*h*0.14+h*0.27;ctx.globalAlpha=p.b;ctx.fillStyle="rgba(210,215,235,0.5)";ctx.beginPath();ctx.arc(px,py,p.s,0,Math.PI*2);ctx.fill();});ctx.globalAlpha=1;ctx.restore();
DUST.forEach(d=>{d.x+=d.dx;d.y+=d.dy;if(d.x<0||d.x>1)d.dx*=-1;if(d.y<0||d.y>1)d.dy*=-1;ctx.globalAlpha=d.a;ctx.fillStyle="rgba(180,200,240,0.5)";ctx.beginPath();ctx.arc(d.x*w,d.y*h,d.r,0,Math.PI*2);ctx.fill();});ctx.globalAlpha=1;
STARS.forEach(s=>{const tw=s.a+Math.sin(t*s.sp*60+s.off)*0.25;ctx.globalAlpha=Math.max(0.02,Math.min(1,tw));const col=starCol(s.temp);ctx.fillStyle=`rgba(${col},0.9)`;ctx.beginPath();ctx.arc(s.x*w,s.y*h,s.r,0,Math.PI*2);ctx.fill();if(s.r>1.05&&tw>0.5){ctx.globalAlpha=tw*0.18;ctx.strokeStyle=`rgba(${col},0.6)`;ctx.lineWidth=0.3;const sx=s.x*w,sy=s.y*h,l=s.r*3.5;ctx.beginPath();ctx.moveTo(sx-l,sy);ctx.lineTo(sx+l,sy);ctx.stroke();ctx.beginPath();ctx.moveTo(sx,sy-l);ctx.lineTo(sx,sy+l);ctx.stroke();}});ctx.globalAlpha=1;
SHOOTING.forEach(ss=>{if(!ss.active&&Math.random()<0.0006){ss.active=true;ss.x=Math.random()*w*0.8+w*0.1;ss.y=Math.random()*h*0.4;const a=Math.PI*0.12+Math.random()*0.35,sp=3.5+Math.random()*4.5;ss.vx=Math.cos(a)*sp;ss.vy=Math.sin(a)*sp;ss.life=0;ss.maxLife=25+Math.random()*45;ss.trail=[];}if(ss.active){ss.trail.push([ss.x,ss.y]);if(ss.trail.length>15)ss.trail.shift();ss.x+=ss.vx;ss.y+=ss.vy;ss.life++;if(ss.life>ss.maxLife){ss.active=false;ss.trail=[];return;}const al=1-ss.life/ss.maxLife;for(let i=1;i<ss.trail.length;i++){ctx.globalAlpha=(i/ss.trail.length)*al*0.5;ctx.strokeStyle="#dce4ff";ctx.lineWidth=1.5*(i/ss.trail.length);ctx.beginPath();ctx.moveTo(ss.trail[i-1][0],ss.trail[i-1][1]);ctx.lineTo(ss.trail[i][0],ss.trail[i][1]);ctx.stroke();}ctx.globalAlpha=al*0.85;ctx.fillStyle="#fff";ctx.beginPath();ctx.arc(ss.x,ss.y,1.3,0,Math.PI*2);ctx.fill();ctx.globalAlpha=1;}});
for(let i=5;i>=1;i--){const ag=ctx.createRadialGradient(cx,cy,radius*(0.92+i*0.012),cx,cy,radius*(1+i*0.075));ag.addColorStop(0,`rgba(55,135,255,${0.04/i})`);ag.addColorStop(0.5,`rgba(35,95,220,${0.02/i})`);ag.addColorStop(1,"transparent");ctx.beginPath();ctx.arc(cx,cy,radius*(1+i*0.075),0,Math.PI*2);ctx.fillStyle=ag;ctx.fill();}
const sa=ctx.createRadialGradient(cx-radius*0.65,cy-radius*0.55,radius*0.15,cx-radius*0.2,cy-radius*0.2,radius*1.2);sa.addColorStop(0,"rgba(130,190,255,0.035)");sa.addColorStop(0.5,"rgba(80,140,255,0.012)");sa.addColorStop(1,"transparent");ctx.beginPath();ctx.arc(cx,cy,radius*1.2,0,Math.PI*2);ctx.fillStyle=sa;ctx.fill();
ctx.save();ctx.beginPath();path({type:"Sphere"});ctx.clip();
for(let pole=0;pole<2;pole++){const lb=pole===0?68:-68;for(let band=0;band<5;band++){const pts=[];for(let i=0;i<=24;i++){const lon=(i/24)*360-180+t*3,lat=lb+Math.sin(i*0.5+t*0.8+band*1.3)*5+band*1.5;const p=proj([lon,lat]);if(p)pts.push(p);}if(pts.length>5){ctx.beginPath();ctx.moveTo(pts[0][0],pts[0][1]);for(let i=1;i<pts.length-1;i++){const xc=(pts[i][0]+pts[i+1][0])/2,yc=(pts[i][1]+pts[i+1][1])/2;ctx.quadraticCurveTo(pts[i][0],pts[i][1],xc,yc);}ctx.strokeStyle=`hsla(${pole===0?140+band*15:160+band*20},65%,55%,${0.022-band*0.004})`;ctx.lineWidth=3.5-band*0.5;ctx.lineCap="round";ctx.stroke();}}}ctx.restore();
ctx.beginPath();path({type:"Sphere"});const oG=ctx.createRadialGradient(cx-radius*0.4,cy-radius*0.38,radius*0.02,cx+radius*0.15,cy+radius*0.15,radius*1.05);oG.addColorStop(0,"#1e7292");oG.addColorStop(0.1,"#1a6888");oG.addColorStop(0.3,"#135878");oG.addColorStop(0.55,"#0c4462");oG.addColorStop(0.8,"#07324c");oG.addColorStop(1,"#041e34");ctx.fillStyle=oG;ctx.fill();
ctx.save();ctx.beginPath();path({type:"Sphere"});ctx.clip();const ssG=ctx.createRadialGradient(cx-radius*0.35,cy-radius*0.3,0,cx-radius*0.1,cy-radius*0.1,radius*0.6);ssG.addColorStop(0,"rgba(40,180,200,0.04)");ssG.addColorStop(0.3,"rgba(30,140,180,0.02)");ssG.addColorStop(1,"transparent");ctx.fillStyle=ssG;ctx.beginPath();ctx.arc(cx,cy,radius,0,Math.PI*2);ctx.fill();
for(let i=0;i<10;i++){const ph=t*0.35+i*0.95;const rx=cx+Math.cos(ph*0.7+i)*radius*0.28,ry=cy+Math.sin(ph*0.5+i*1.3)*radius*0.28;const rg=ctx.createRadialGradient(rx,ry,0,rx,ry,radius*0.1);rg.addColorStop(0,`rgba(100,200,240,${0.012+Math.sin(ph)*0.006})`);rg.addColorStop(1,"transparent");ctx.fillStyle=rg;ctx.beginPath();ctx.arc(rx,ry,radius*0.1,0,Math.PI*2);ctx.fill();}ctx.restore();
ctx.beginPath();path(d3.geoGraticule().step([15,15])());ctx.strokeStyle="rgba(80,150,200,0.02)";ctx.lineWidth=0.25;ctx.stroke();
if(worldData?.features){worldData.features.forEach(f=>{const name=f.properties?.name||"";const isH=hovered===name;const ppl=(records.people?.[name]||[]).length,plc=(records.places?.[name]||[]).length,has=ppl+plc;ctx.beginPath();path(f);let fill;if(isH)fill="#b5c8a0";else if(has>8)fill="#d4a030";else if(has>4)fill="#c09538";else if(has>0)fill="#a88540";else fill=lc(name);ctx.fillStyle=fill;ctx.fill();const cent=d3.geoCentroid(f),pp=proj(cent);if(pp&&!isNaN(pp[0])){ctx.save();ctx.beginPath();path(f);ctx.clip();const eg=ctx.createRadialGradient(pp[0]-4,pp[1]-5,0,pp[0],pp[1],50);eg.addColorStop(0,"rgba(255,255,240,0.035)");eg.addColorStop(0.5,"rgba(255,255,230,0.008)");eg.addColorStop(1,"rgba(0,0,0,0.02)");ctx.fillStyle=eg;ctx.fill();ctx.restore();}ctx.beginPath();path(f);if(isH){ctx.strokeStyle="rgba(255,255,255,0.6)";ctx.lineWidth=1.8;ctx.stroke();ctx.save();ctx.shadowColor="rgba(200,230,180,0.3)";ctx.shadowBlur=10;ctx.stroke();ctx.restore();}else{ctx.strokeStyle=`rgba(180,210,230,${has?0.12:0.06})`;ctx.lineWidth=0.25;ctx.stroke();}if(has>0&&!isH){ctx.beginPath();path(f);ctx.save();ctx.shadowColor=`rgba(212,160,48,${Math.min(0.4,has*0.05)})`;ctx.shadowBlur=8;ctx.strokeStyle=`rgba(212,160,48,${Math.min(0.3,has*0.04)})`;ctx.lineWidth=1.4;ctx.stroke();ctx.restore();}});}
// ‚îÄ‚îÄ Sub-national borders + hover highlight ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
if(subData?.features){
  ctx.save();ctx.beginPath();path({type:"Sphere"});ctx.clip();
  subData.features.forEach(f=>{
    const sname=f.properties?.name||"";
    const isH=hovered===sname;
    const ppl=(records.people?.[sname]||[]).length;
    const plc=(records.places?.[sname]||[]).length;
    const has=ppl+plc;
    if(isH){
      // Hovered fill ‚Äî same green tint as country hover
      ctx.beginPath();path(f);
      ctx.fillStyle="#b5c8a0";ctx.fill();
      // Bright border + glow
      ctx.beginPath();path(f);
      ctx.strokeStyle="rgba(255,255,255,0.65)";ctx.lineWidth=1.6;ctx.stroke();
      ctx.save();ctx.shadowColor="rgba(200,230,180,0.35)";ctx.shadowBlur=10;
      ctx.beginPath();path(f);ctx.stroke();
      ctx.restore();
    } else if(has>0){
      // Gold glow border for regions with entries
      ctx.beginPath();path(f);
      ctx.save();
      ctx.shadowColor=`rgba(212,160,48,${Math.min(0.35,has*0.06)})`;
      ctx.shadowBlur=7;
      ctx.strokeStyle=`rgba(212,160,48,${Math.min(0.28,has*0.05)})`;
      ctx.lineWidth=1.2;ctx.stroke();
      ctx.restore();
    }
  });
  // Dotted internal borders on top
  ctx.setLineDash([2,4]);ctx.lineWidth=0.55;
  ctx.strokeStyle="rgba(220,230,255,0.16)";
  subData.features.forEach(f=>{ctx.beginPath();path(f);ctx.stroke();});
  ctx.setLineDash([]);
  ctx.restore();
}
// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
// ‚îÄ‚îÄ Place pins ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
{
  // ‚îÄ‚îÄ Colors ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
  const PIN_E={base:"#75AADB",mid:"#4a8cc4",dark:"#1e4f82",ring:"rgba(255,210,80,0.9)",glow:"rgba(117,170,219,0.6)"};
  const PIN_S={base:"#5577cc",mid:"#2244aa",dark:"#0a1a5c",ring:"rgba(255,255,255,0.85)",glow:"rgba(85,119,204,0.6)"};

  // ‚îÄ‚îÄ Visibility: must be meaningfully on the front face ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
  function pinVisible(lon,lat){
    const lam=(lon+rot[0])*Math.PI/180;
    const phi=lat*Math.PI/180;
    return Math.cos(phi)*Math.cos(lam)>0.12; // tighter threshold ‚Äî avoids limb distortion
  }

  // ‚îÄ‚îÄ Find a land-confirmed point for a given lon/lat/country ‚îÄ
  // Tries the exact point first, then spirals outward toward
  // the country centroid if the point is over water.
  const _landCache=new Map();
  function snapToLand(lon,lat,country){
    const key=`${lon.toFixed(2)},${lat.toFixed(2)},${country}`;
    if(_landCache.has(key))return _landCache.get(key);

    // Find the country/sub-national feature to test against
    let feat=null;
    if(subData?.features){
      feat=subData.features.find(f=>(f.properties?.name||"")===country)||null;
    }
    if(!feat&&worldData?.features){
      feat=worldData.features.find(f=>(f.properties?.name||"")===country)||null;
    }

    let result=[lon,lat];

    if(feat){
      // Test the exact point
      if(d3.geoContains(feat,[lon,lat])){
        _landCache.set(key,result);
        return result;
      }
      // Spiral toward centroid in 12 steps
      const cent=d3.geoCentroid(feat);
      for(let s=1;s<=12;s++){
        const t2=s/12;
        const tlon=lon+(cent[0]-lon)*t2;
        const tlat=lat+(cent[1]-lat)*t2;
        if(d3.geoContains(feat,[tlon,tlat])){
          result=[tlon,tlat];
          break;
        }
      }
      // If still not found, use centroid itself
      if(result[0]===lon&&result[1]===lat&&!d3.geoContains(feat,[lon,lat])){
        result=[cent[0],cent[1]];
      }
    }

    _landCache.set(key,result);
    return result;
  }

  // ‚îÄ‚îÄ Find spread positions confirmed over land ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
  // For clusters at same coords, fan outward in geo-space and
  // test each candidate ‚Äî only use confirmed-land positions.
  function spreadPositions(baseLon,baseLat,count,country){
    if(count===1)return[[baseLon,baseLat]];
    const positions=[];
    // Try fanning in different directions at increasing radii
    const step=count<=3?2.5:count<=5?2.0:1.5;
    const angles=[0,Math.PI,Math.PI*0.5,Math.PI*1.5,Math.PI*0.25,Math.PI*0.75,Math.PI*1.25];
    let found=0;
    let radius=step;
    const tried=new Set();
    while(found<count&&radius<15){
      for(let ai=0;ai<angles.length&&found<count;ai++){
        const tlon=baseLon+Math.cos(angles[ai])*radius;
        const tlat=baseLat+Math.sin(angles[ai])*radius*0.6;
        const norm=[((tlon+180)%360)-180, Math.max(-89,Math.min(89,tlat))];
        const k=`${norm[0].toFixed(1)},${norm[1].toFixed(1)}`;
        if(tried.has(k))continue;
        tried.add(k);
        const snapped=snapToLand(norm[0],norm[1],country);
        positions.push(snapped);
        found++;
      }
      radius+=step;
    }
    // Pad if needed
    while(positions.length<count)positions.push([baseLon,baseLat]);
    return positions;
  }

  // ‚îÄ‚îÄ The new spectacular pin ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
  function drawPin(px,py,col,r,pulse){
    ctx.save();

    const spike=r*2.2; // taller, more elegant spike
    const headY=0;     // head centre at translated origin

    // Translate so the spike TIP is at (px, py) ‚Äî pin tip anchors to the place
    ctx.translate(px, py-spike);

    // ‚îÄ‚îÄ 1. Outer pulsing aura (time-based, baked into `pulse` 0‚Üí1) ‚îÄ‚îÄ
    const auraR=r*(1.6+pulse*0.7);
    const aura=ctx.createRadialGradient(0,headY,r*0.5,0,headY,auraR);
    aura.addColorStop(0,col.glow.replace("0.6",`${0.25+pulse*0.15}`));
    aura.addColorStop(0.5,col.glow.replace("0.6","0.08"));
    aura.addColorStop(1,"transparent");
    ctx.beginPath();ctx.arc(0,headY,auraR,0,Math.PI*2);
    ctx.fillStyle=aura;ctx.fill();

    // ‚îÄ‚îÄ 2. Spike ‚Äî sleek tapered teardrop ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
    ctx.beginPath();
    ctx.moveTo(-r*0.48, headY+r*0.1);
    ctx.bezierCurveTo(-r*0.28, headY+r*0.4, -r*0.08, spike*0.75, 0, spike);
    ctx.bezierCurveTo( r*0.08, spike*0.75,  r*0.28, headY+r*0.4, r*0.48, headY+r*0.1);
    ctx.closePath();
    const spG=ctx.createLinearGradient(-r*0.5,headY,r*0.5,spike);
    spG.addColorStop(0,col.mid);
    spG.addColorStop(0.4,col.base);
    spG.addColorStop(1,col.dark);
    ctx.fillStyle=spG;
    ctx.fill();
    // Spike highlight edge
    ctx.strokeStyle="rgba(255,255,255,0.18)";ctx.lineWidth=0.5;ctx.stroke();

    // ‚îÄ‚îÄ 3. Pin head ‚Äî rich metallic sphere illusion ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
    // Outer glow ring
    const ringR=r*1.22;
    ctx.beginPath();ctx.arc(0,headY,ringR,0,Math.PI*2);
    const ringG=ctx.createRadialGradient(0,headY,r*0.7,0,headY,ringR);
    ringG.addColorStop(0,"transparent");
    ringG.addColorStop(0.7,col.ring.replace("0.9","0.25"));
    ringG.addColorStop(1,col.ring.replace("0.9","0.55"));
    ctx.fillStyle=ringG;ctx.fill();

    // Main head body
    ctx.beginPath();ctx.arc(0,headY,r,0,Math.PI*2);
    const hg=ctx.createRadialGradient(-r*0.38,headY-r*0.38,r*0.02, 0,headY,r*1.05);
    hg.addColorStop(0,"rgba(255,255,255,0.7)");
    hg.addColorStop(0.18,col.base);
    hg.addColorStop(0.55,col.mid);
    hg.addColorStop(1,col.dark);
    ctx.fillStyle=hg;ctx.fill();

    // Crisp white rim
    ctx.beginPath();ctx.arc(0,headY,r,0,Math.PI*2);
    ctx.strokeStyle=col.ring;
    ctx.lineWidth=Math.max(0.8,r*0.12);
    ctx.stroke();

    // ‚îÄ‚îÄ 4. Specular highlight ‚Äî top-left glint ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
    ctx.beginPath();
    ctx.arc(-r*0.28, headY-r*0.28, r*0.32, 0, Math.PI*2);
    const sg=ctx.createRadialGradient(-r*0.32,headY-r*0.32,0, -r*0.28,headY-r*0.28, r*0.32);
    sg.addColorStop(0,"rgba(255,255,255,0.72)");
    sg.addColorStop(0.5,"rgba(255,255,255,0.22)");
    sg.addColorStop(1,"transparent");
    ctx.fillStyle=sg;ctx.fill();

    // ‚îÄ‚îÄ 5. Inner pip ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
    ctx.beginPath();ctx.arc(0,headY,r*0.22,0,Math.PI*2);
    ctx.fillStyle="rgba(255,255,255,0.95)";ctx.fill();

    // ‚îÄ‚îÄ 6. Ground anchor dot at spike tip ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
    ctx.beginPath();ctx.arc(0,spike,r*0.18,0,Math.PI*2);
    ctx.fillStyle=col.ring;ctx.fill();

    ctx.restore();
  }

  // ‚îÄ‚îÄ Collect + resolve all pins ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
  const allPins=[];
  // Group entries by base coordinate so we can spread same-location clusters in geo-space
  const byCoord=new Map();
  Object.entries(records.places||{}).forEach(([country,entries])=>{
    entries.slice(0,7).forEach(entry=>{
      let lon=entry.lon??null, lat=entry.lat??null;
      if(lon===null||lat===null){
        const cities=CITY_DB[country]||[];
        const match=cities.find(c=>c[0]===entry.name);
        if(match){lon=match[1];lat=match[2];}
        else if(worldData?.features){
          const feat=worldData.features.find(f=>(f.properties?.name||"")===country);
          if(feat){const c=d3.geoCentroid(feat);lon=c[0];lat=c[1];}
        }
      }
      if(lon===null||lat===null)return;
      // Snap to land immediately
      const safe=snapToLand(lon,lat,country);
      const ck=`${safe[0].toFixed(2)},${safe[1].toFixed(2)},${country}`;
      if(!byCoord.has(ck))byCoord.set(ck,{lon:safe[0],lat:safe[1],country,entries:[]});
      byCoord.get(ck).entries.push({who:entry.who});
    });
  });

  // For each coord group, generate spread positions over land
  byCoord.forEach(({lon,lat,country,entries})=>{
    const positions=spreadPositions(lon,lat,entries.length,country);
    entries.forEach((e,i)=>{
      allPins.push({lon:positions[i][0],lat:positions[i][1],who:e.who,country});
    });
  });

  // ‚îÄ‚îÄ Render ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
  ctx.save();
  ctx.beginPath();path({type:"Sphere"});ctx.clip();

  const pinSize=Math.max(5, radius*0.026);
  const pulse=(Math.sin(t*2.2)+1)*0.5; // smooth 0‚Üí1 oscillation for aura

  // Sort back-to-front by screen Y so southern pins render over northern ones
  const toRender=[];
  allPins.forEach(pin=>{
    if(!pinVisible(pin.lon,pin.lat))return;
    const pp=proj([pin.lon,pin.lat]);
    if(!pp||isNaN(pp[0]))return;
    toRender.push({sx:pp[0],sy:pp[1],who:pin.who});
  });
  toRender.sort((a,b)=>a.sy-b.sy);

  toRender.forEach(p=>{
    const col=p.who==="eissa"?PIN_E:PIN_S;
    drawPin(p.sx,p.sy,col,pinSize,pulse);
  });

  ctx.restore();
}
// ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
ctx.save();ctx.beginPath();path({type:"Sphere"});ctx.clip();[76,-76].forEach(lb=>{for(let i=0;i<=36;i++){const lon=(i/36)*360-180,lat=lb+Math.sin(i*0.8+t*0.1)*3;const p=proj([lon,lat]);if(!p)return;const ig=ctx.createRadialGradient(p[0],p[1],0,p[0],p[1],radius*0.04);ig.addColorStop(0,"rgba(230,240,255,0.1)");ig.addColorStop(0.6,"rgba(210,225,245,0.04)");ig.addColorStop(1,"transparent");ctx.fillStyle=ig;ctx.beginPath();ctx.arc(p[0],p[1],radius*0.04,0,Math.PI*2);ctx.fill();}});ctx.restore();
ctx.save();ctx.beginPath();path({type:"Sphere"});ctx.clip();LIGHTS.forEach(cl=>{const p=proj([cl.lon,cl.lat]);if(!p||isNaN(p[0]))return;const dx=(p[0]-cx)/radius,dy=(p[1]-cy)/radius;const dot=dx*-0.5+dy*-0.45;if(dot>-0.05)return;const dark=Math.min(1,Math.abs(dot+0.05)*2.2);const fl=cl.b+Math.sin(t*cl.fl*60+cl.off)*0.12;ctx.globalAlpha=dark*fl*0.6;const lg=ctx.createRadialGradient(p[0],p[1],0,p[0],p[1],cl.r*3);lg.addColorStop(0,"rgba(255,228,140,0.8)");lg.addColorStop(0.3,"rgba(255,200,90,0.3)");lg.addColorStop(1,"transparent");ctx.fillStyle=lg;ctx.beginPath();ctx.arc(p[0],p[1],cl.r*3,0,Math.PI*2);ctx.fill();});ctx.globalAlpha=1;ctx.restore();
ctx.save();ctx.beginPath();path({type:"Sphere"});ctx.clip();CLOUDS.forEach(cc=>{const blon=cc.lon+t*cc.speed*40;cc.puffs.forEach(pf=>{const p=proj([blon+pf.dlon,cc.lat+pf.dlat]);if(!p||isNaN(p[0]))return;const pr=radius*pf.r;ctx.fillStyle=`rgba(0,0,20,${pf.op*0.25})`;ctx.beginPath();ctx.arc(p[0]+1.5,p[1]+1.5,pr*1.2,0,Math.PI*2);ctx.fill();const cg=ctx.createRadialGradient(p[0]-pr*0.25,p[1]-pr*0.25,0,p[0],p[1],pr);cg.addColorStop(0,`rgba(255,255,255,${pf.op*1.2})`);cg.addColorStop(0.5,`rgba(240,245,255,${pf.op*0.6})`);cg.addColorStop(1,"transparent");ctx.fillStyle=cg;ctx.beginPath();ctx.arc(p[0],p[1],pr,0,Math.PI*2);ctx.fill();});});ctx.restore();
ctx.beginPath();path({type:"Sphere"});const sG=ctx.createRadialGradient(cx-radius*0.5,cy-radius*0.45,radius*0.02,cx+radius*0.25,cy+radius*0.25,radius*1.2);sG.addColorStop(0,"rgba(255,252,235,0.09)");sG.addColorStop(0.12,"rgba(255,248,225,0.04)");sG.addColorStop(0.38,"rgba(0,0,0,0)");sG.addColorStop(0.6,"rgba(0,0,15,0.1)");sG.addColorStop(0.78,"rgba(0,0,12,0.25)");sG.addColorStop(0.92,"rgba(0,0,10,0.42)");sG.addColorStop(1,"rgba(0,0,8,0.55)");ctx.fillStyle=sG;ctx.fill();
ctx.beginPath();path({type:"Sphere"});const tG=ctx.createLinearGradient(cx-radius*0.6,cy-radius*0.5,cx+radius*0.6,cy+radius*0.5);tG.addColorStop(0,"transparent");tG.addColorStop(0.38,"transparent");tG.addColorStop(0.44,"rgba(220,130,60,0.02)");tG.addColorStop(0.48,"rgba(240,120,80,0.025)");tG.addColorStop(0.52,"rgba(220,90,70,0.02)");tG.addColorStop(0.56,"rgba(180,80,100,0.015)");tG.addColorStop(0.62,"transparent");tG.addColorStop(1,"transparent");ctx.fillStyle=tG;ctx.fill();
const spX=cx-radius*0.33,spY=cy-radius*0.31;const spG=ctx.createRadialGradient(spX,spY,0,spX,spY,radius*0.25);spG.addColorStop(0,"rgba(255,255,255,0.055)");spG.addColorStop(0.4,"rgba(255,255,255,0.018)");spG.addColorStop(1,"transparent");ctx.beginPath();ctx.arc(spX,spY,radius*0.25,0,Math.PI*2);ctx.fillStyle=spG;ctx.fill();
ctx.beginPath();path({type:"Sphere"});const fr=ctx.createRadialGradient(cx,cy,radius*0.82,cx,cy,radius*1.01);fr.addColorStop(0,"transparent");fr.addColorStop(0.7,"transparent");fr.addColorStop(0.88,"rgba(60,120,240,0.03)");fr.addColorStop(0.95,"rgba(80,160,255,0.06)");fr.addColorStop(1,"rgba(100,180,255,0.09)");ctx.fillStyle=fr;ctx.fill();
ctx.beginPath();path({type:"Sphere"});const fr2=ctx.createRadialGradient(cx,cy,radius*0.96,cx,cy,radius*1.02);fr2.addColorStop(0,"transparent");fr2.addColorStop(0.5,"rgba(255,100,80,0.012)");fr2.addColorStop(1,"rgba(255,80,60,0.025)");ctx.fillStyle=fr2;ctx.fill();
ctx.beginPath();path({type:"Sphere"});ctx.strokeStyle="rgba(75,155,255,0.08)";ctx.lineWidth=1.5;ctx.stroke();
const fx0=cx-radius*1.15,fy0=cy-radius*0.95;for(let i=0;i<4;i++){const fd=0.3+i*0.22;const fx=fx0+(cx-fx0)*fd*2,fy=fy0+(cy-fy0)*fd*2,fR=(4-i)*radius*0.03+Math.sin(t*0.5+i)*2;const fg=ctx.createRadialGradient(fx,fy,0,fx,fy,fR);fg.addColorStop(0,`rgba(180,210,255,${0.035-i*0.007})`);fg.addColorStop(1,"transparent");ctx.fillStyle=fg;ctx.beginPath();ctx.arc(fx,fy,fR,0,Math.PI*2);ctx.fill();}
const vig=ctx.createRadialGradient(cx,cy,Math.min(w,h)*0.25,cx,cy,Math.max(w,h)*0.75);vig.addColorStop(0,"transparent");vig.addColorStop(0.7,"transparent");vig.addColorStop(1,"rgba(0,0,0,0.35)");ctx.fillStyle=vig;ctx.fillRect(0,0,w,h);
if(frame%3===0){ctx.globalAlpha=0.4;const pat=ctx.createPattern(grainC,"repeat");ctx.fillStyle=pat;ctx.fillRect(0,0,w,h);ctx.globalAlpha=1;}
})(0);
}

// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê GATE ANIMATIONS ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
function startGateAnimations(){
  const fwC=$("FIREWORKS"),fwCtx=fwC.getContext("2d");
  const cfC=$("CONFETTI"),cfCtx=cfC.getContext("2d");
  const mrC=$("MATE_RAIN"),mrCtx=mrC.getContext("2d");
  function resizeGate(){fwC.width=cfC.width=mrC.width=innerWidth;fwC.height=cfC.height=mrC.height=innerHeight;}
  resizeGate();addEventListener("resize",resizeGate);

  const rockets=[],particles=[],confetti=[],mateDrops=[];
  const argColors=["#75AADB","#FFFFFF","#FFD700","#FFA500","#75AADB","#fff","#87CEEB","#FFD700"];

  function launchRocket(){rockets.push({x:Math.random()*innerWidth,y:innerHeight,vx:(Math.random()-0.5)*4,vy:-(Math.random()*7+9),life:0,maxLife:35+Math.random()*25,color:argColors[Math.floor(Math.random()*argColors.length)]});}
  function explode(x,y,color){const count=50+Math.floor(Math.random()*40);for(let i=0;i<count;i++){const angle=Math.random()*Math.PI*2,speed=Math.random()*6+2;particles.push({x,y,vx:Math.cos(angle)*speed,vy:Math.sin(angle)*speed,life:0,maxLife:50+Math.random()*40,color:Math.random()>0.25?color:argColors[Math.floor(Math.random()*argColors.length)],r:Math.random()*2.8+1});}}

  // More confetti
  for(let i=0;i<120;i++){confetti.push({x:Math.random()*innerWidth,y:Math.random()*innerHeight-innerHeight,w:Math.random()*10+4,h:Math.random()*5+2,color:argColors[Math.floor(Math.random()*argColors.length)],vy:Math.random()*2.5+1,vx:(Math.random()-0.5)*2,rot:Math.random()*360,rotSpeed:Math.random()*10-5,wobble:Math.random()*Math.PI*2});}

  // Mate emoji rain
  const mateEmojis=["üßâ","‚öΩ","‚òÄÔ∏è","ü•©","üèÜ","üíÉ","üéµ","ü´ï","ü•ü","üéÜ","‚≠ê","üåø"];
  for(let i=0;i<35;i++){mateDrops.push({x:Math.random()*innerWidth,y:Math.random()*-innerHeight,emoji:mateEmojis[Math.floor(Math.random()*mateEmojis.length)],vy:Math.random()*1.5+0.5,vx:(Math.random()-0.5)*0.8,size:Math.random()*18+14,rot:Math.random()*360,rotSpeed:Math.random()*3-1.5,opacity:Math.random()*0.4+0.15});}

  // Floating emojis from bottom

  // ‚îÄ‚îÄ Photo fireworks ‚Äî FRONT AND CENTER, MASSIVE ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
  const PHOTOS=[
    {src:"data:image/jpeg;base64,",w:400,h:267,label:"Buenos Aires"},
    {src:"data:image/jpeg;base64,",w:225,h:400,label:"La Bombonera"},
    {src:"data:image/jpeg;base64,",w:276,h:400,label:"Los Andes"},
    {src:"data:image/jpeg;base64,",w:269,h:400,label:"El Diego"},
    {src:"data:image/jpeg;base64,",w:400,h:225,label:"Folklore"},
  ];
  const photoRain=document.getElementById("PHOTO_RAIN");
  let _photoIdx=0; // cycle through all 5 on initial burst

  function launchPhoto(forcedIdx){
    if(!photoRain||gate.classList.contains("gone"))return;
    const idx=forcedIdx!==undefined?forcedIdx:Math.floor(Math.random()*PHOTOS.length);
    const p=PHOTOS[idx];

    // Scale: anywhere from 0.6x (tiny ghost) to 2.8x (massive) ‚Äî heavily random
    const scaleRoll=Math.random();
    const scale = scaleRoll<0.15 ? 0.5+Math.random()*0.4       // tiny 15%
                : scaleRoll<0.5  ? 1.2+Math.random()*0.6        // medium 35%
                :                  1.9+Math.random()*0.9;        // huge 50%

    const w=Math.round(p.w*scale), h=Math.round(p.h*scale);
    const img=document.createElement("img");
    img.src=p.src; img.width=w; img.height=h;
    const el=document.createElement("div"); el.className="photo-rocket";
    el.appendChild(img);

    // x: full width including partial off-screen edges
    const startX=(Math.random()*110)-5;
    const tiltStart=(Math.random()-0.5)*50;
    const tiltEnd=(Math.random()-0.5)*40;
    const drift=(Math.random()-0.5)*220;

    // opacity: some ghostly (0.35), most vivid (0.9-1.0)
    const opacity=Math.random()<0.2 ? 0.25+Math.random()*0.25 : 0.82+Math.random()*0.18;

    // speed: some rockets (fast 900ms), most medium, some lazy (4500ms)
    const riseRoll=Math.random();
    const riseDuration = riseRoll<0.12 ? 700+Math.random()*500
                       : riseRoll<0.55 ? 1800+Math.random()*1200
                       :                 3200+Math.random()*1800;

    // pop-in spin: some barely rotate, some spin wildly
    const spinIn=Math.random()<0.15 ? `rotate(${(Math.random()-0.5)*720}deg)` : `rotate(${tiltStart*0.4}deg)`;

    el.style.cssText=`left:${startX}%;bottom:-${h+30}px;opacity:0;transform:scale(0.3) rotate(${tiltStart}deg);transition:none;`;
    photoRain.appendChild(el);

    requestAnimationFrame(()=>{
      el.style.transition=`transform 0.3s cubic-bezier(0.16,1,0.3,1), opacity 0.2s ease`;
      el.style.opacity=String(opacity);
      el.style.transform=`scale(1) ${spinIn}`;
      const fadeDelay=Math.max(0, riseDuration-600);
      setTimeout(()=>{
        el.style.transition=`bottom ${riseDuration}ms cubic-bezier(${Math.random()*0.3},${Math.random()},${0.5+Math.random()*0.5},1), `
          +`transform ${riseDuration}ms ease, `
          +`opacity 500ms ease ${fadeDelay}ms`;
        const targetBottom=innerHeight*(0.5+Math.random()*0.7);
        el.style.bottom=targetBottom+"px";
        el.style.transform=`translateX(${drift}px) scale(${0.6+Math.random()*0.5}) rotate(${tiltEnd}deg)`;
        el.style.opacity="0";
        setTimeout(()=>el.remove(), riseDuration+700);
      }, 60);
    });
  }

  // Continuous interval ‚Äî random gap 400ms‚Äì2200ms, random burst size
  function scheduleNext(){
    if(!gate||gate.classList.contains("gone"))return;
    const gap=400+Math.random()*1800;
    setTimeout(()=>{
      if(gate.classList.contains("gone"))return;
      // Burst size: 1 (60%), 2 (28%), 3 (12%)
      const burst=Math.random()<0.6?1:Math.random()<0.7?2:3;
      for(let i=0;i<burst;i++){
        setTimeout(()=>launchPhoto(), i*(120+Math.random()*300));
      }
      scheduleNext();
    }, gap);
  }

  // Immediate staggered burst ‚Äî one of each photo so all 5 appear quickly
  [0,1,2,3,4].forEach((i,idx)=>setTimeout(()=>launchPhoto(idx), 150+idx*280));
  // Also sprinkle a few random ones in the first 3 seconds
  [600,1100,1700,2400,3000].forEach(t=>setTimeout(()=>launchPhoto(), t));
  // Start the continuous random scheduler after 1s
  setTimeout(scheduleNext, 1000);
  // ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

  const emojiPool=["üßâ","‚öΩ","ü•©","‚òÄÔ∏è","üéµ","üíÉ","üèÜ","ü´ï","üåü","üéÜ","ü•ü","‚≠ê","üî•","üéâ","ü™ò","üé∏","üêÑ","üåø"];
  const gate=$("GATE");
  function spawnEmoji(){
    if(!gate||gate.classList.contains("gone"))return;
    const el=document.createElement("div");el.className="gate-float";
    el.textContent=emojiPool[Math.floor(Math.random()*emojiPool.length)];
    el.style.left=Math.random()*94+3+"%";el.style.bottom="-50px";
    el.style.fontSize=(Math.random()*24+18)+"px";
    el.style.animationDuration=(Math.random()*4+3)+"s";
    gate.appendChild(el);setTimeout(()=>el.remove(),8000);
  }
  const emojiInterval=setInterval(spawnEmoji,180);

  let fwTimer=0;
  function animateGate(){
    if(!gate||gate.style.display==="none"){clearInterval(emojiInterval);return;}
    requestAnimationFrame(animateGate);fwTimer++;
    if(fwTimer%25===0)launchRocket();
    if(fwTimer%40===0)launchRocket();
    if(fwTimer%60===0)launchRocket();

    // Fireworks
    fwCtx.clearRect(0,0,fwC.width,fwC.height);
    for(let i=rockets.length-1;i>=0;i--){const r=rockets[i];r.x+=r.vx;r.y+=r.vy;r.vy+=0.15;r.life++;fwCtx.globalAlpha=0.7;fwCtx.fillStyle=r.color;fwCtx.beginPath();fwCtx.arc(r.x,r.y,2.5,0,Math.PI*2);fwCtx.fill();fwCtx.globalAlpha=0.25;const tg=fwCtx.createRadialGradient(r.x,r.y,0,r.x,r.y,10);tg.addColorStop(0,r.color);tg.addColorStop(1,"transparent");fwCtx.fillStyle=tg;fwCtx.beginPath();fwCtx.arc(r.x,r.y,10,0,Math.PI*2);fwCtx.fill();if(r.life>r.maxLife||r.vy>-1){explode(r.x,r.y,r.color);rockets.splice(i,1);}}
    for(let i=particles.length-1;i>=0;i--){const p=particles[i];p.x+=p.vx;p.y+=p.vy;p.vx*=0.98;p.vy*=0.98;p.vy+=0.06;p.life++;const al=1-p.life/p.maxLife;fwCtx.globalAlpha=al;fwCtx.fillStyle=p.color;fwCtx.beginPath();fwCtx.arc(p.x,p.y,p.r*al,0,Math.PI*2);fwCtx.fill();fwCtx.globalAlpha=al*0.3;const pg=fwCtx.createRadialGradient(p.x,p.y,0,p.x,p.y,p.r*4);pg.addColorStop(0,p.color);pg.addColorStop(1,"transparent");fwCtx.fillStyle=pg;fwCtx.beginPath();fwCtx.arc(p.x,p.y,p.r*4,0,Math.PI*2);fwCtx.fill();if(p.life>p.maxLife)particles.splice(i,1);}
    fwCtx.globalAlpha=1;

    // Confetti
    cfCtx.clearRect(0,0,cfC.width,cfC.height);
    confetti.forEach(c=>{c.y+=c.vy;c.x+=c.vx+Math.sin(c.wobble+=0.03)*0.6;c.rot+=c.rotSpeed;if(c.y>cfC.height+20){c.y=-20;c.x=Math.random()*cfC.width;}cfCtx.save();cfCtx.translate(c.x,c.y);cfCtx.rotate(c.rot*Math.PI/180);cfCtx.globalAlpha=0.75;cfCtx.fillStyle=c.color;cfCtx.fillRect(-c.w/2,-c.h/2,c.w,c.h);cfCtx.restore();});

    // Mate emoji rain
    mrCtx.clearRect(0,0,mrC.width,mrC.height);
    mateDrops.forEach(m=>{m.y+=m.vy;m.x+=m.vx+Math.sin(fwTimer*0.02+m.rot)*0.3;m.rot+=m.rotSpeed;if(m.y>mrC.height+30){m.y=-40;m.x=Math.random()*mrC.width;m.emoji=mateEmojis[Math.floor(Math.random()*mateEmojis.length)];}mrCtx.save();mrCtx.translate(m.x,m.y);mrCtx.rotate(m.rot*Math.PI/180);mrCtx.globalAlpha=m.opacity;mrCtx.font=m.size+"px serif";mrCtx.textAlign="center";mrCtx.textBaseline="middle";mrCtx.fillText(m.emoji,0,0);mrCtx.restore();});
  }
  animateGate();
}

// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê GATE ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
function checkGate(){
  try{if(sessionStorage.getItem("atlas-auth")==="1"){$("GATE").style.display="none";bootApp();return;}}catch(e){}
  startGateAnimations();
  const pin=$("GPIN"),btn=$("GBTN"),err=$("GERR");
  function tryCode(){
    const v=pin.value.trim().toLowerCase();
    if(!v){err.textContent="üßâ‚ùì";return;}
    if(v===GATE_CODE.toLowerCase()){
      try{sessionStorage.setItem("atlas-auth","1");}catch(e){}
      err.textContent="";$("GATE").classList.add("gone");
      setTimeout(()=>{$("GATE").style.display="none";bootApp();},1200);
    }else{
      err.textContent="üö´ No. üö´";
      pin.style.animation="gateShake 0.6s ease";
      pin.style.borderColor="rgba(255,50,50,0.6)";
      setTimeout(()=>{pin.style.animation="";pin.style.borderColor="";},700);
    }
  }
  btn.onclick=tryCode;pin.onkeydown=e=>{if(e.key==="Enter")tryCode();};pin.focus();
}

// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê INIT ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
async function bootApp(){
  $("LS").style.display="flex";resize();
  usingSupa=initSupabase();
  if(usingSupa){const ok=await loadFromSupabase();if(ok){subscribeRealtime();}else{usingSupa=false;await loadLocal();$("sync").className="off";$("sync-label").textContent="local only";}}else{await loadLocal();$("sync").className="";$("sync-label").textContent="local";}
  rS();
  const SUB_GEO="https://raw.githubusercontent.com/nvkelso/natural-earth-vector/master/geojson/ne_10m_admin_1_states_provinces.geojson";
  Promise.all([fetch(GEO).then(r=>r.json()),fetch(SUB_GEO).then(r=>r.json()).catch(()=>null)]).then(([world,sub])=>{worldData=world;if(sub){subData={type:"FeatureCollection",features:sub.features.filter(f=>{const iso=f.properties?.iso_a2||"";const sov=f.properties?.sovereignt||f.properties?.admin||"";return iso==="US"||iso==="CA"||iso==="AR"||sov==="United States of America"||sov==="Canada"||sov==="Argentina";})}}; $("LS").classList.add("h");startR();}).catch(()=>$("LS").classList.add("h"));
}
checkGate();
</script>
</body>
</html>
